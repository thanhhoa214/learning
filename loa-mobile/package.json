{
  "name": "loa-mobile",
  "version": "0.0.1",
  "author": "Thanh Hoa Nguyen",
  "description": "An Interior Design project with Ionic",
  "homepage": "https://abc.com/",
  "license": "MIT",
  "scripts": {
    "postinstall": "jetifier",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test:ci": "ng test --c=ci",
    "e2e": "ng e2e",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --ext .ts --fix",
    "sync": "npx cap sync",
    "run:ios": "npm run build:prod && npx cap sync ios && npx cap open ios",
    "run:android": "npm run build:prod && npx cap sync android && npx cap open android",
    "live-reload:ios": "ionic capacitor run ios -l --external",
    "live-reload:android": "ionic capacitor run android -l --external",
    "deploy:ios": "cd $PWD/ios/App && gem install bundler && bundle install && bundle exec fastlane beta",
    "deploy:android": "cd $PWD/android && ./gradlew assembleDebug",
    "build-deploy:ios": "npm run build:prod && npm run sync && npm run deploy:ios",
    "open:ios": "npx cap open ios",
    "open:android": "npx cap open android",
    "gen:graphql": "graphql-codegen",
    "gen:ngxs": "echo In loa-mobile, your path to folder: && read path && echo Your store name: && read name && ngxs --directory src/app/$path --name $name --folder-name store --spec false",
    "gen:splashscreen": "cordova-res android --resources resources --skip-config --copy  && cordova-res ios --resources resources --skip-config --copy",
    "native-run:ios": "native-run ios --app ios/App"
  },
  "private": true,
