{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "4.3.1",
    "generators": {
      "v2.0": {
        "generatorName": "typescript-angular",
        "inputSpec": "https://avc-api.azurewebsites.net/swagger/v1/swagger.yaml",
        "output": "#{cwd}/libs/shared/api/src/lib/generated",
        "additionalProperties": {
          "ngVersion": "11.0.0",
          "supportsES6": "true",
          "withInterfaces": true,
          "apiModulePrefix": "Open",
          "fileNaming": "kebab-case",
          "useSingleRequestParameter": true,
          "providedIn": "none"
        }
      }
    }
  }
}
